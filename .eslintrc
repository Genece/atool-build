{
  "extends": "eslint-config-airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-console": [0],
    "no-underscore-dangle": [0],
    "no-param-reassign": ["error", { "props": false }],
    "space-before-function-paren": [2, "never"],
    "max-len": [2, 120, 2]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
}
